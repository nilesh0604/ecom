# Staging Environment Variables
# Copy this file to .env.staging and fill in the values

# Database
DB_PASSWORD=staging_secure_password_change_me

# Redis
REDIS_PASSWORD=redis_staging_password_change_me

# JWT
JWT_SECRET=staging_jwt_secret_at_least_32_characters_long

# CORS
CORS_ORIGIN=http://staging.yourdomain.com

# Stripe (use test keys for staging)
STRIPE_SECRET_KEY=sk_test_your_stripe_test_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Email (use Mailhog for staging)
SMTP_HOST=mailhog
SMTP_PORT=1025
SMTP_USER=
SMTP_PASS=
FROM_EMAIL=noreply@staging.yourdomain.com

# pgAdmin
PGADMIN_EMAIL=admin@staging.local
PGADMIN_PASSWORD=pgadmin_password_change_me

# Grafana
GRAFANA_PASSWORD=grafana_password_change_me
