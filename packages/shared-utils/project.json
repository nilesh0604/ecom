{
  "name": "shared-utils",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/shared-utils/src",
  "tags": ["scope:shared", "type:utils"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src/",
        "cwd": "packages/shared-utils"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/shared-utils"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest",
        "cwd": "packages/shared-utils"
      }
    },
    "test:run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "packages/shared-utils"
      }
    }
  }
}
